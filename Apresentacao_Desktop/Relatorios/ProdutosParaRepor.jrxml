	<queryString language="SQL">
		<![CDATA[SELECT `marketPlus`.estoque.`QUANTIDADEMINIMADESEJADA`,
	SUM( `marketPlus`.lote.`EMESTOQUE`) AS SOMAESTOQUE,
	SUM( `marketPlus`.lote.`NASPRATELEIRAS`) AS SOMAPRATELEIRAS,
	SUM( `marketPlus`.lote.`EMESTOQUE` + `marketPlus`.lote.`NASPRATELEIRAS`) AS QUANTIDADETOTAL,
	`marketPlus`.produto.`NOME`
FROM `marketPlus`.lote
	INNER JOIN `marketPlus`.estoque ON 
	 `marketPlus`.lote.estoque_id = `marketPlus`.estoque.`ID` 
	INNER JOIN `marketPlus`.produto ON 
	 `marketPlus`.produto.estoque_id = `marketPlus`.lote.estoque_id 
GROUP BY `marketPlus`.estoque.`ID`
HAVING 
	 SOMAPRATELEIRAS + SOMAESTOQUE  < `marketPlus`.estoque.`QUANTIDADEMINIMADESEJADA`]]>
	</queryString>
